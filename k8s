parameters:
  - name: imageName
    in: query
    description: "Name of the image. Supports both old and new Artifactory URLs."
    required: true
    schema:
      type: string
    examples:
      existingUrl:
        summary: Existing Artifactory URL format
        value: "https://old-artifactory.example.com/images/sample-image.jpg"
      newUrl:
        summary: New Artifactory URL format
        value: "https://new-artifactory.example.com/images/sample-image.jpg"
